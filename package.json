    "@vue/cli-plugin-typescript": "^3.9.0",
    "@vue/cli-service": "^3.9.3",
    "lint-staged": "^9.2.1",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "prettier-stylelint": "^0.4.2",
    "sass": "^1.22.7",
    "sass-loader": "^7.1.0",
    "stylelint": "^15.10.1",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.9.2",
